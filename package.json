{
  "name": "node_advanced",
  "version": "1.0.0",
  "main": "index.js",
  "scripts": {
    "start": "node dist/main",
    "build": "rimraf dist && npx tsc -p tsconfig-build.json",
    "lint": "eslint .",
    "lint:fix": "npm run lint -- --fix",
    "test": "jest --passWithNoTests --no-cache --runInBand",
    "test:watch": "npm t -- --watch",
    "test:verbose": "jest --passWithNoTests --runInBand",
    "test:staged": "npm t -- --findRelatedTests",
    "test:coverage": "npm t -- --coverage",
    "test:fb-api": "npm t -- -c ./jest.integration.config.js ./tests/external/facebook_api.test.ts",
    "typeorm": "node --require ts-node/register ./node_modules/typeorm/cli.js -d ormconfig.ts"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "description": "",
  "devDependencies": {
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/jest": "^29.5.12",
    "@types/jsonwebtoken": "^9.0.6",
    "@types/uuid": "^10.0.0",
    "jest": "^29.7.0",
    "jest-mock-extended": "^3.0.7",
    "pg-mem": "^3.0.2",
    "ts-jest": "^29.2.5",
    "typescript": "^5.5.4"
  },
  "engines": {
    "node": "20.x"
  },
  "dependencies": {
    "@types/module-alias": "^2.0.4",
    "@types/node": "^22.5.1",
    "axios": "^1.7.7",
    "cors": "^2.8.5",
    "eslint-config-love": "^63.0.0",
    "express": "^4.21.0",
    "jsonwebtoken": "^9.0.2",
    "module-alias": "^2.2.3",
    "pg": "^8.12.0",
    "reflect-metadata": "^0.2.2",
    "rimraf": "^6.0.1",
    "typeorm": "^0.3.20",
    "uuid": "^10.0.0"
  }
}
